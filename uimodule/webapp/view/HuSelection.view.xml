    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true">

    <Panel>
        <Table width="auto">
            <columns>
                <Column>
                    <Text text="{i18n>columnName}" />
                </Column>
                <Column>
                    <Text text="{i18n>columnSurname}" />
                </Column>
                <Column>
                    <Text text="{i18n>columnDescription}" />
                </Column>
            </columns>

            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="Simon"/>
                        <Text text="Abt"/>
                        <Text text="Bester"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Panel>

    <Button text="{i18n>testButton}"
